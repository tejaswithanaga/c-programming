#include<stdio.h>
int main()
{
 int a[3][3]={1,2,3,4,5,6,7,8,9};
 int i,j,avg,sum=0;
 for(i=0;i<3;i++)
 {
 for(j=0;j<3;j++)
 
 {
 printf("%d\t",a[i][j]);
 }
 printf("\n");
 }
 for(i=0;i<3;i++)
 {
 for(j=0;j<3;j++)
 {
 sum+=a[i][j];
 }
 }
 avg=sum/9;
 printf("sum of elements in matrix=%d\n",sum);
 printf("average of elements in matrix=%d",avg);
 return 0;
}
