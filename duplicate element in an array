#include <stdio.h>
int main() {
 int i, j, k;
 int a[5] = {1, 2, 2, 3, 4};
 int n = 5;
 for (i = 0; i < n; i++) {
 for (j = i + 1; j < n;) 
 {
 if (a[i] == a[j]) {
 for (k = j; k < n - 1; k++) 
 {
 a[k] = a[k + 1];
 }
 n--;
 } 
 else
 {
 j++;
 }
 }
 }
 printf("Array without duplicates: ");
 for (i = 0; i < n; i++)
 {
 printf("%d ", a[i]);
 }
}
