#include<stdio.h>
int main()
{
 int median,mode;
 float mean;
 int a[]={1,2,3,7,5,6,90,2,36};
 int n=sizeof(a)/sizeof(int);
 int sum=0;
 for(int i=0;i<n-1;i++)
 {
 for(int j=0;j<n-i-1;j++)
 {
 if(a[j]>a[j+1])
 {
 int temp=a[j];
 a[j]=a[j+1];
 a[j+1]=temp;
 }
 }
 }
 printf("After acsending: ");
 for(int i=0;i<n;i++)
 {
 printf("%d,",a[i]);
 }
 
 for(int i=0;i<n;i++)
 {
 sum+=a[i];
 }
 mean= (float)sum/n;
 printf("\nmean=%.2f\n",mean);
 for(int i=0;i<n;i++)
 {
 median=a[n/2];
 }
 printf("median=%d\n",median);
 
 int maxcount=0;
 for(int i=0;i<n;i++)
 {
 int count=0;
 for(int j=0;j<n;j++)
 {
 if(a[j]==a[i])
 count++;
 }
 if (count>maxcount)
 {
 maxcount=count;
 mode=a[i];
 }
 }
 printf("mode=%d",mode);
 }
